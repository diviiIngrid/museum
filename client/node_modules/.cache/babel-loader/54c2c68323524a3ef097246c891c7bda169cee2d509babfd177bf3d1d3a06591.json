lient/src/pages/AboutPage/AboutPage.js"],"sourcesContent":["import React, { useEffect } from 'react';\r\nimport './AboutPage.css';\r\n\r\nconst AboutPage = () => {\r\n\r\n    useEffect(() => {\r\n        window.scrollTo(0, 0);\r\n      }, []);\r\n\r\n  return (\r\n    <div className=\"about-page\">\r\n      <section className=\"about-hero\">\r\n        <div className=\"container\">\r\n          <div className=\"about-hero-content\">\r\n            <h1>О музее</h1>\r\n            <p>Национальный музей имени Алдан-Маадыр — центр истории, культуры и духовности Тувы</p>\r\n          </div>\r\n        </div>\r\n      </section>\r\n\r\n      <section className=\"about-section\">\r\n        <div className=\"container\">\r\n          <h2 className=\"about-section-title\">О музее</h2>\r\n          <p className=\"about-text\">\r\n            13 мая 1929 г. на заседании Политбюро ЦК ТНРП, где присутствовали члены тувинского правительства и консульства СССР было решено о создании краеведческого музея тувинского народа. Директором первого в истории тувинского музея был назначен Владимир Петрович Ермолаев (1892-1982 гг.), профессиональный фотограф, снимавший на свою пленку природу Тувы, быт населения, занятия, промыслы, различные моменты государственной и общественной жизни. Он оставил после себя свыше 4 тысяч негативов и фотографий краеведческой тематики от Тувы феодальной до Тувы социалистической, которые в настоящее время составляют «золотой» фонд музея.\r\nВ создании музея в Туве большую помощь оказали советские научные и культурные учреждения. Зоологическим музеем Академии Наук СССР тувинскому музею были подарены чучела птиц, млекопитающих, пресмыкающихся, морских беспозвоночных. Минералогическим музеем выделена коллекция минералов, впоследствии пригодившаяся при определении местных материалов. Географическим обществом безвозмездно был передан набор аппаратуры с самопишущими приборами для организации метеорологической станции при музее. Выделили материалы из своих фондов Центральный музей имени В.И. Ленина, Государственный Исторический музей, Эрмитаж, Русский музей, книгоиздательства и другие учреждения.\r\n          </p>\r\n        </div>\r\n      </section>\r\n\r\n      <section className=\"about-section\">\r\n        <div className=\"container\">\r\n          <h2 className=\"about-section-title\">История музея</h2>\r\n          <p className=\"about-text\">\r\n            Музей был основан в начале XX века и с тех пор стал одним из важнейших культурных учреждений \r\n            Республики Тыва. В его фондах хранятся уникальные археологические находки, этнографические \r\n            экспонаты и произведения искусства.\r\n          </p>\r\n        </div>\r\n      </section>\r\n\r\n      <section className=\"about-section contact-info\">\r\n        <div className=\"container\">\r\n          <h2 className=\"about-section-title\">Контакты</h2>\r\n          <p><strong>Адрес:</strong> Республика Тыва, г. Кызыл, ул. Титова 30, 667000</p>\r\n          <p><strong>Электронная почта:</strong> tuva-museum@mail.ru</p>\r\n          <p><strong>Телефон:</strong> +7 (39422) XX-XXX</p>\r\n        </div>\r\n      </section>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default AboutPage;\r\n"],"mappings":";;AAAA,OAAOA,KAAK,IAAIC,SAAS,QAAQ,OAAO;AACxC,OAAO,iBAAiB;AAAC,SAAAC,MAAA,IAAAC,OAAA;AAEzB,MAAMC,SAAS,GAAGA,CAAA,KAAM;EAAAC,EAAA;EAEpBJ,SAAS,CAAC,MAAM;IACZK,MAAM,CAACC,QAAQ,CAAC,CAAC,EAAE,CAAC,CAAC;EACvB,CAAC,EAAE,EAAE,CAAC;EAEV,oBACEJ,OAAA;IAAKK,SAAS,EAAC,YAAY;IAAAC,QAAA,gBACzBN,OAAA;MAASK,SAAS,EAAC,YAAY;MAAAC,QAAA,eAC7BN,OAAA;QAAKK,SAAS,EAAC,WAAW;QAAAC,QAAA,eACxBN,OAAA;UAAKK,SAAS,EAAC,oBAAoB;UAAAC,QAAA,gBACjCN,OAAA;YAAAM,QAAA,EAAI;UAAO;YAAAC,QAAA,EAAAC,YAAA;YAAAC,UAAA;YAAAC,YAAA;UAAA,OAAI,CAAC,eAChBV,OAAA;YAAAM,QAAA,EAAG;UAAiF;YAAAC,QAAA,EAAAC,YAAA;YAAAC,UAAA;YAAAC,YAAA;UAAA,OAAG,CAAC;QAAA;UAAAH,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OACrF;MAAC;QAAAH,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OACH;IAAC;MAAAH,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAA